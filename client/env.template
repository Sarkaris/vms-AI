# React App Environment Variables
# These are used during build time
# Copy this file to .env in the client directory

# Socket.io Server URL (optional - auto-detected in production)
# Only set if you need a custom socket URL
REACT_APP_SOCKET_URL=

# API Base URL (optional - uses relative paths by default)
# Only set if API is on a different domain
REACT_APP_API_URL=

# Environment
REACT_APP_ENV=production

